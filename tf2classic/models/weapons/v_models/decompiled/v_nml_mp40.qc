	{ event 6002 1 "3" }
	fadein 0.2
	fadeout 0.2
	node "raised"
	snap
	fps 30
}

$sequence "holster" {
	"v_nml_mp40_anims\holster.smd"
	activity "ACT_IDLE_TO_LOWERED" 1
	fadein 0.2
	fadeout 0.2
	transition "raised" "lowered"
	fps 30
}

$sequence "holster_idle" {
	"v_nml_mp40_anims\holster_idle.smd"
	activity "ACT_VM_IDLE_LOWERED" 1
	fadein 0.2
